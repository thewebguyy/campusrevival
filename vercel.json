{
    "version": 2,
    "builds": [
        {
            "src": "campus-revival-backend/app.js",
            "use": "@vercel/node"
        }
    ],
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/campus-revival-backend/app.js"
        },
        {
            "source": "/(.*)",
            "destination": "/frontend/$1"
        }
    ]
}
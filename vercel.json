{
    "version": 2,
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "/api/:path*"
        },
        {
            "source": "/",
            "destination": "/frontend/index.html"
        },
        {
            "source": "/:path*",
            "destination": "/frontend/:path*"
        }
    ]
}
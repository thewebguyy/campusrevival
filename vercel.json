{
    "version": 2,
    "cleanUrls": true,
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/campus-revival-backend/app.js"
        },
        {
            "source": "/((?!api/|campus-revival-backend/).*)",
            "destination": "/frontend/$1"
        },
        {
            "source": "/",
            "destination": "/frontend/index.html"
        }
    ]
}